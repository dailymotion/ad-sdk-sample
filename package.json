{
  "name": "ad-sdk-sample",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://dailymotion.github.io/ad-sdk-sample/",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && npm run copy-files",
